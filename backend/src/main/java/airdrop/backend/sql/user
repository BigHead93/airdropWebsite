CREATE TABLE IF NOT EXISTS `user`(
  `id` INT UNSIGNED AUTO_INCREMENT,
  `name` VARCHAR(20) NOT NULL DEFAULT '' COMMENT 'user name',
  `email` VARCHAR(50) NOT NULL DEFAULT '' COMMENT 'email address',
  `psw` VARCHAR(250) NOT NULL DEFAULT '' COMMENT 'encrypted PASSWORD',
  `created_at` DATETIME NOT NULL DEFAULT '1970-01-01 00:00:00' COMMENT 'create time',
  `note` VARCHAR(250) NOT NULL DEFAULT '' COMMENT 'note',
  PRIMARY KEY (`id`)
)ENGINE=INNODB DEFAULT CHARSET=utf8;